<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <ADUsesOwinOrOpenIdConnect>False</ADUsesOwinOrOpenIdConnect>
    <PublishProvider>AzureWebSite</PublishProvider>
    <LastUsedBuildConfiguration>Debug</LastUsedBuildConfiguration>
    <LastUsedPlatform>Any CPU</LastUsedPlatform>
    <SiteUrlToLaunchAfterPublish>http://diversityapi.azurewebsites.net</SiteUrlToLaunchAfterPublish>
    <LaunchSiteAfterPublish>True</LaunchSiteAfterPublish>
    <ExcludeApp_Data>False</ExcludeApp_Data>
    <MSDeployServiceURL>diversityapi.scm.azurewebsites.net:443</MSDeployServiceURL>
    <DeployIisAppPath>DiversityAPI</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <EnableMSDeployBackup>False</EnableMSDeployBackup>
    <UserName>$DiversityAPI</UserName>
    <_SavePWD>True</_SavePWD>
    <_DestinationType>AzureWebSite</_DestinationType>
    <PublishDatabaseSettings>
      <Objects>
        <ObjectGroup Name="Collection" Order="1" Enabled="False">
          <Destination Path="Data Source={0},{1};Initial Catalog={2};User ID={3};Password={4};Application Name=EntityFramework" Name="Data Source={0},{1};Initial Catalog={2};User ID={3};Password={4};MultipleActiveResultSets=True;Application Name=EntityFramework" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source={0},{1};Initial Catalog={2};User ID={3};Password={4};Application Name=EntityFramework" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\Collection_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="metadata=res://*/Collection.DiversityCollection.csdl|res://*/Collection.DiversityCollection.ssdl|res://*/Collection.DiversityCollection.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source={0},{1};initial catalog={2};user id={3};password={4};MultipleActiveResultSets=True;App=EntityFramework&quot;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
        <ObjectGroup Name="Unknown" Order="3" Enabled="False">
          <Destination Path="Data Source={0},{1};Initial Catalog={2};User ID={3};Password={4};Application Name=EntityFramework" Name="Data Source={0},{1};Initial Catalog={2};User ID={3};Password={4};MultipleActiveResultSets=True;Application Name=EntityFramework" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source={0},{1};Initial Catalog={2};User ID={3};Password={4};Application Name=EntityFramework" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\Unknown_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="metadata=res://*/Module.Module.csdl|res://*/Module.Module.ssdl|res://*/Module.Module.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source={0},{1};initial catalog={2};user id={3};password={4};MultipleActiveResultSets=True;App=EntityFramework&quot;" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
        <ObjectGroup Name="TaxonNames" Order="5" Enabled="False" xmlns="">
          <Destination Path="Data Source={0},{1};Initial Catalog={2};User ID={3};Password={4};Application Name=EntityFramework" Name="Data Source={0},{1};Initial Catalog={2};User ID={3};Password={4};MultipleActiveResultSets=True;Application Name=EntityFramework" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="DiversityService.DB.TaxonNames.TaxonNames, DiversityService.Collection" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="DiversityService.DB.Module.Module" Order="6" Enabled="False" xmlns="">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="DiversityService.DB.Module.Module, DiversityService.Collection" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="DiversityService.DB.Collection.DiversityCollection" Order="7" Enabled="False" xmlns="">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="DiversityService.DB.Collection.DiversityCollection, DiversityService.Collection" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="UserDB" Order="8" Enabled="False" xmlns="">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="DiversityService.API.WebHost.Models.ApplicationDbContext, DiversityService.API.WebHost" MigrationConfiguration="DiversityService.API.WebHost.Migrations.Configuration, DiversityService.API.WebHost" Origin="Configuration" />
          </Object>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)Collection-Web.config Connection String">
      <ParameterValue>metadata=res://*/Collection.DiversityCollection.csdl|res://*/Collection.DiversityCollection.ssdl|res://*/Collection.DiversityCollection.msl;provider=System.Data.SqlClient;provider connection string="data source={0},{1};initial catalog={2};user id={3};password={4};multipleactiveresultsets=True;application name=EntityFramework;App=EntityFramework"</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DiversityService.DB.Collection.DiversityCollection-Web.config Connection String" />
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DiversityService.DB.Module.Module-Web.config Connection String" />
    <MSDeployParameterValue Include="$(DeployParameterPrefix)TaxonNames-Web.config Connection String">
      <ParameterValue>metadata=res://*/TaxonNames.DiversityTaxonNames.csdl|res://*/TaxonNames.DiversityTaxonNames.ssdl|res://*/TaxonNames.DiversityTaxonNames.msl;provider=System.Data.SqlClient;provider connection string="Data Source={0},{1};Initial Catalog={2};User ID={3};Password={4};MultipleActiveResultSets=True;Application Name=EntityFramework"</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)Unknown-Web.config Connection String">
      <ParameterValue>metadata=res://*/Module.Module.csdl|res://*/Module.Module.ssdl|res://*/Module.Module.msl;provider=System.Data.SqlClient;provider connection string="data source={0},{1};initial catalog={2};user id={3};password={4};multipleactiveresultsets=True;application name=EntityFramework;App=EntityFramework"</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)UserDB-Web.config Connection String" />
  </ItemGroup>
  <ItemGroup>
    <_ConnectionStringsToInsert Include="DiversityService.DB.Collection.DiversityCollection" />
    <_ConnectionStringsToInsert Include="DiversityService.DB.Module.Module" />
  </ItemGroup>
</Project>